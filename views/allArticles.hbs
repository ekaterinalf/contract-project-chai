{{#each articles}}
<div class="card mb-3 content teablock container col-md-4">
  <img src="{{this.img}}" class="card-img-top tea_img" alt="...">

  <div class="card-body">
    <h5 class="card-title">{{this.nameTea}}</h5>
    <h5 class="card-title">{{this.region}}</h5>
    <p class="card-text">{{this.description}}</p>
  </div>
</div>
{{/each}}
